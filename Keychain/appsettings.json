{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "MongoDB": {
    "ConnectionString": "",
    "DatabaseName": "redbox-keychain",
    "ChatsKeysCollection": "ChatsKeys",
    "GroupsKeysCollection": "GroupsKeys",
    "SupervisorsMasterKeysCollection": "SupervisorsMasterKeys",
    "SupervisedChatsKeysCollection": "SupervisedChatsKeys",
    "SupervisedGroupsKeysCollection": "SupervisedGroupsKeys",
    "SupervisorPrivateKeyCollection": "SupervisorPrivateKey",
    "SupervisorPublicKeyCollection": "SupervisorPublicKey",
    "UsersMasterKeysCollection": "UsersMasterKeys",
    "UsersPrivateKeysCollection": "UsersPrivateKeys",
    "UsersPublicKeysCollection": "UsersPublicKeys"
  },
  "Redis": {
    "ConnectionString": "",
    "SessionDatabaseIndex": 0,
    "UsernameTokenDatabaseIndex": 1
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http1AndHttp2"
    },
    "Endpoints": {
      "Http": {
        "Url": "http://*:5400",
        "Protocols": "Http2"
      }
    }
  }
}