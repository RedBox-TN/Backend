<Project Sdk="Microsoft.NET.Sdk.Web">

    <PropertyGroup>
        <TargetFramework>net7.0</TargetFramework>
        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
    </PropertyGroup>

    <ItemGroup>
        <Protobuf Include="Protos\service.proto" GrpcServices="Server"/>
        <Protobuf Include="Protos\request.proto" GrpcServices="Server"/>
        <Protobuf Include="Protos\response.proto" GrpcServices="Server"/>
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="Grpc.AspNetCore" Version="2.52.0"/>
        <PackageReference Include="MongoDB.Driver" Version="2.19.1"/>
        <PackageReference Include="StackExchange.Redis" Version="2.6.104"/>
    </ItemGroup>

    <ItemGroup>
        <ProjectReference Include="..\RedBoxAuth\RedBoxAuth.csproj"/>
    </ItemGroup>

</Project>
