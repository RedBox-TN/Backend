{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "RedBoxDB": {
    "ConnectionString": "mongodb://redboxdev:27017",
    "DatabaseName": "redbox",
    "ChatsCollection": "Chats"
  },
  "UsersDB": {
    "ConnectionString": "mongodb://redboxdev:27017",
    "DatabaseName": "redbox",
    "UsersCollection": "Users",
    "RolesCollection": "Roles"
  },
  "Redis": {
    "ConnectionString": "redboxdev"
  },
  "AuthenticationOptions": {
    "MaxLoginAttempts": 3,
    "PendingAuthMinutes": 5,
    "SessionExpireMinutes": 30,
    "TokenSizeBytes": 12,
    "CacheExpirationScanMinutes": 5,
    "TotpSharedSecretSize": 16,
    "TotpIssuer": "RedBox",
    "HashSaltSize": 16,
    "Argon2IdDegreeOfParallelism": 8,
    "Argon2IdIterations": 2,
    "Argon2IdMemorySize": 2000,
    "Argon2IdHashSize": 128,
    "Argon2IdPepper": "-FpbJ3>Iesy@i#UeRVO$"
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  }
}
