{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "RedBoxDB": {
    "ConnectionString": "mongodb://redboxdev:27017",
    "Database": "redbox",
    "ChatsCollection": "Chats",
    "GroupsCollection": "Groups",
    "ChatsDatabase": "redbox-chats",
    "SupervisedChatsDatabase": "redbox-supervisedChats",
    "GroupsDatabase": "redbox-groups",
    "SupervisedGroupsDatabase": "redbox-supervisedGroups"
  },
  "UsersDB": {
    "ConnectionString": "mongodb://redboxdev:27017",
    "DatabaseName": "redbox-accounts",
    "UsersCollection": "Users",
    "RolesCollection": "Roles"
  },
  "Redis": {
    "ConnectionString": "redboxdev"
  },
  "SecurityOptions": {
    "MaxLoginAttempts": 3,
    "PendingAuthMinutes": 5,
    "SessionExpireMinutes": 30,
    "TokenSizeBytes": 12,
    "CacheExpirationScanMinutes": 15,
    "TotpSharedSecretSize": 16,
    "TotpIssuer": "RedBox",
    "HashSaltSize": 16,
    "Argon2IdDegreeOfParallelism": 16,
    "Argon2IdIterations": 40,
    "Argon2IdMemorySize": 8192,
    "Argon2IdHashSize": 256,
    "Argon2IdPepper": "U!2)q?{4?tmnQM2Vz4(3"
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  }
}
