<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net7.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <RootNamespace>RedBoxAuth</RootNamespace>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="GoogleAuthenticator" Version="3.0.0"/>
        <PackageReference Include="Grpc.AspNetCore" Version="2.52.0"/>
        <PackageReference Include="Konscious.Security.Cryptography.Argon2" Version="1.3.0"/>
        <PackageReference Include="MemoryPack" Version="1.9.13"/>
        <PackageReference Include="MongoDB.Driver" Version="2.19.1"/>
        <PackageReference Include="StackExchange.Redis" Version="2.6.104"/>
        <PackageReference Include="Standart.Hash.xxHash" Version="4.0.5"/>
        <PackageReference Include="ZstdSharp.Port" Version="0.7.1"/>
    </ItemGroup>

    <ItemGroup>
        <Protobuf Include="Protos\authentication.proto" GrpcServices="Server"/>
    </ItemGroup>

</Project>
